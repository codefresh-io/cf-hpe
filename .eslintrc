{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "settings": {
    "import/resolver": {
      "babel-module-alias": {}
    }
  },
  "rules": {
    "arrow-body-style": ["off"],
    "no-underscore-dangle": ["off"],
    "no-use-before-define": ["error", { "functions": false, "classes": true }]
  }
}
